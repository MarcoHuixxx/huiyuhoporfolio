    <div class="container">
        <div class="row">

            <div class="col-sm-6 col-lg-4">
                <div class="contact-info-item">
                    <h3>
                        <%=pageText.address%>:
                    </h3>
                    <ul>
                        <li>
                            <a href="#">
                                <%=pageText.address1%>
                            </a>
                        </li>
                        <li>
                            <a href="#">
                                <%=pageText.address2%>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>

            <div class="col-sm-6 col-lg-4">
                <div class="contact-info-item active">
                    <h3>
                        <%=pageText.contactMethods%>:
                    </h3>
                    <ul>
                        <li>
                            <span>
                                <%=pageText.phone%>:
                            </span>
                            <a href="tel:85264366837">852-64366837</a>
                        </li>
                        <li>
                            <span>
                                <%=pageText.whatsapp%>:
                            </span>
                            <a href="<%=pageText.whatsappApiLink%>" target="_blank">852-69965503</a>
                        </li>

                    </ul>
                </div>
            </div>

            <div class="col-sm-6 offset-sm-3 offset-lg-0 col-lg-4">
                <div class="contact-info-item">
                    <h3>
                        <%=pageText.email%>:
                    </h3>
                    <ul>
                        <li>
                            <a href="mailto:hello@jeel.com">zoefacebeauty@gmail.com</a>
                        </li>

                    </ul>
                </div>
            </div>

        </div>
    </div>
</div>
<!-- End Contact Info -->

<!-- Contact Form -->
<div class="contact-form-area pb-100">
    <div class="container">
        <div class="form-item">
            <div class="contact-shape">
                <img src="../../assets/images/contact-shape1.png" alt="Shape">
                <img src="../../assets/images/contact-shape2.png" alt="Shape">
            </div>

            <form id="contactForm">
                <div class="section-title two">
                    <h2>
                        <%=pageText.getInTouch%>
                    </h2>
                </div>
                <div class="row">

                    <div class="col-lg-6">
                        <div class="form-group">
                            <input type="text" name="name" id="name" class="form-control" placeholder="Name" required
                                data-error="<%=pageText.enterName%>">
                            <div class="help-block with-errors"></div>
                        </div>
                    </div>

                    <div class="col-lg-6">
                        <div class="form-group">
                            <input type="email" name="email" id="email" class="form-control" placeholder="Email"
                                required data-error="<%=pageText.enterEmail%>">
                            <div class="help-block with-errors"></div>
                        </div>
                    </div>

                    <div class="col-lg-6">
                        <div class="form-group">
                            <input type="text" name="phone_number" id="phone_number" placeholder="Phone" required
                                data-error="<%=pageText.enterPhone%>" class="form-control">
                            <div class="help-block with-errors"></div>
                        </div>
                    </div>

                    <div class="col-lg-6">
                        <div class="form-group">
                            <input type="text" name="msg_subject" id="msg_subject" class="form-control"
                                placeholder="Subject" required data-error="<%=pageText.enterSubject%>">
                            <div class="help-block with-errors"></div>
                        </div>
                    </div>

                    <div class="col-lg-12">
                        <div class="form-group">
                            <textarea name="message" class="form-control" id="message" cols="30" rows="8"
                                placeholder="Write message" required data-error="<%=pageText.enterMessage%>"></textarea>
                            <div class="help-block with-errors"></div>
                        </div>
                    </div>

                    <div class="col-lg-12 col-md-12">
                        <div class="form-group">
                            <div class="form-check agree-label">
                                <input name="gridCheck" value="I agree to the terms and privacy policy."
                                    class="form-check-input" type="checkbox" id="gridCheck" required>
                                <label class="form-check-label" for="gridCheck">
                                    Accept <a href="terms-conditions.html">
                                        <%=pageText.termsConditions%>
                                    </a> and <a href="privacy-policy.html">
                                        <%=pageText.privacyPolicy%>
                                    </a>
                                </label>
                                <div class="help-block with-errors gridCheck-error"></div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-12">
                        <div class="text-center">
                            <button type="submit" class="btn common-btn two">
                                <%=pageText.sendMessage%>
                            </button>
                            <div id="msgSubmit" class="h3 text-center hidden"></div>
                            <div class="clearfix"></div>
                        </div>
                    </div>

                </div>
            </form>

        </div>
    </div>
</div>
<!-- End Contact Form -->

