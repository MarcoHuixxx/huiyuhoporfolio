<!DOCTYPE html>
<html lang="en" dir="lrt">

<head>

  <main>
    <!-- Breadcrumb Area S t a r t -->
    <section class="breadcrumb-section breadcrumb-bg">
      <div class="container">
        <div class="breadcrumb-text">
          <nav aria-label="breadcrumb" class="breadcrumb-nav wow fadeInUp" data-wow-delay="0.0s">
            <ul class="breadcrumb listing">
              <li class="breadcrumb-item single-list"><a href="/" class="single">
                  <%=pageText.home%>
                </a></li>
              <li class="breadcrumb-item single-list" aria-current="page"><a href="javascript:void(0)" class="single">
                  <%=pageText.vouchers%>
                </a></li>
            </ul>
          </nav>
          <h1 class="title wow fadeInUp" data-wow-delay="0.1s">
            <%=pageText.vouchers%>
          </h1>
        </div>
      </div>
    </section>
    <!-- End-of Breadcrumb Area -->


    <!-- About us Area S t a r t -->
    <section class="about-area mt-100">
      <div class="container">
        <div class="row justify-content-between ">
          <div class="col-lg-6 my-auto">
            <!-- Section Tittle -->
            <div class="section-tittle mb-35  bg-light px-5 py-5   shadow-lg  rounded-3">
              <span class="sub-tittle text-capitalize font-600">
                <%=pageText.whatIsVoucher%>
              </span>
              <p class="pera-subtitle mb-15">
                <%=pageText.whatIsVoucherDesc%>
              </p>
              <p class="sub-tittle text-capitalize font-600">
                <%=pageText.target%>
              </p>
              <p class="pera-subtitle">
                <%=pageText.targetDesc%>
              </p>
            </div>

          </div>
          <div class="col-lg-5">
            <div class="position-relative h-100">
              <div class="play-button">
                <a id="play-video-two" class="video-play-button-two d-none d-lg-block d-none d-lg-block"
                  href="javascript:void(0)">
                  <span></span>
                </a>

                <div id="video-overlay" class="video-overlay">
                  <a class="video-overlay-close">&times;</a>
                </div>
              </div>
              <div>
                <img class="w-100 d-none d-lg-block" src="../../../assets/images/gallery/whatisvoucher.png" alt="image">
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
    <!-- End-of About us Area -->

    <!-- helpful area S t a r t-->
    <section class="helpful-area-three section-padding">
      <div class="container">
        <div class="row justify-content-center">
          <div class="col-xl-7">
            <!-- Section Tittle -->
            <div class="section-tittle text-center">
              <span class="sub-tittle text-capitalize font-600">
                <%=pageText.serviceContents%>
              </span>
              <h5 class="title font-500">
                <%=pageText.serviceContentDesc%>
              </h5>
            </div>
          </div>
        </div>
      </div>
      <!-- Gallery S t a r t -->
      <div class="gallery-area mt-10">
        <div class="gallery-slider d-flex">
          <% pageText.serviceGalleryData.forEach((gallery, index)=> { %>
            <div class="">
              <img src="../../../assets/images/gallery/<%=gallery.imageName%>" alt="img">
              <span class="sub-tittle text-capitalize font-600 align-self-center d-flex justify-content-center">
                <%=gallery.name%>
              </span>


            </div>
            <% }) %>

        </div>
      </div>
      <!-- End-of Gallery -->
    </section>
    <!-- End-of helpful-->


    <!-- helpful area S t a r t-->
    <section class="helpful-area-three pb-80">
      <div class="container">
        <div class="row justify-content-center">
          <div class="col-xl-7">
            <!-- Section Tittle -->
            <div class="section-tittle text-center mb-50">
              <span class="sub-tittle text-capitalize font-600">
                <%=pageText.howToApply%>
              </span>
            </div>
          </div>
        </div>

      </div>
      <!-- Gallery S t a r t -->
      <div class="gallery-area mt-10 container">
        <div class="gallery-slider d-flex">
          <% for (let i=0; i < 2; i++) { %>
            <% pageText.howToApplyData.forEach((gallery, index)=> { %>

              <div class="text-center">
                <img src="../../../assets/images/gallery/<%=gallery.imageName%>" alt="img">
                <span class="sub-tittle text-capitalize font-600 align-self-center d-flex justify-content-center">
                  <%=gallery.title%>
                </span>
                <span class=" text-capitalize align-self-center d-flex justify-content-center">
                  <%=gallery.description%>
                </span>

              </div>
              <% }) %>
                <% } %>

        </div>
      </div>
      <!-- End-of Gallery -->
    </section>
    <!-- End-of helpful-->


    <!-- About us Area S t a r t -->
    <section class="about-area mb-15">
      <div class="container">
        <div class="row justify-content-between ">
          <div class="col-lg-6 my-auto">
            <!-- Section Tittle -->

            <div class="section-tittle mb-35  bg-light px-5 py-5   shadow-lg  rounded-3">
              <div class="d-flex ">
                <div class="col-6 col-lg-6 col-md-6 col-sm-6 view-wrapper text-center bg-secondary p-2 ">
                  <div class="case1Container">
                    <span class="">
                      <%=pageText.case1%>
                    </span>

                  </div>
                  <h5 class="sub-tittle text-capitalize font-400">
                    <%=pageText.physio%>
                  </h5>
                  <div class="container-fluid p-1 text-white d-flex align-items-center justify-content-center">

                    <p>
                      <span class="font-300">
                        <%=pageText.serviceHourPerMonth%>
                      </span>
                      <span class="font-300 text-capitalize">
                        <%=pageText.fourHours%>
                      </span>
                    </p>
                  </div>

                </div>
                <div class="col-6 col-lg-6 col-md-6 col-sm-6 view-wrapper text-center bg-success p-2">
                  <h5 class="sub-tittle text-capitalize font-400">
                    <%=pageText.rehab%>
                  </h5>
                  <div class="container-fluid p-1  text-white d-flex align-items-center justify-content-center">
                    <p>
                      <span class="font-300">
                        <%=pageText.serviceHourPerMonth%>
                      </span>
                      <span class="font-300 text-capitalize">
                        <%=pageText.fourHours%>
                      </span>
                    </p>
                  </div>

                </div>


              </div>

              <div class="col-12 view-wrapper text-center p-2 mt-2" style="background-color:floralwhite">
                <h5 class="sub-tittle text-capitalize font-400">
                  <%=pageText.case1Fee%>
                </h5>
                <div class="container-fluid p-1 text-white d-flex align-items-center justify-content-center">
                  <h6 class="font-300">
                    <%=pageText.case1MonthFee%>
                  </h6>

                  </h6>
                </div>

              </div>

              <div class="d-flex mt-5 border ">
                <div class="case1Container">
                  <span class="">
                    <%=pageText.case2%>
                  </span>

                </div>
                <div class="col-6 col-lg-6 col-md-6 col-sm-6 view-wrapper text-center bg-secondary p-2">
                  <h5 class="sub-tittle text-capitalize font-400">
                    <%=pageText.physio%>
                  </h5>
                  <div class="container-fluid p-1 text-white d-flex align-items-center justify-content-center">
                    <p>
                      <span class="font-300">
                        <%=pageText.serviceHourPerMonth%>
                      </span>
                      <span class="font-300 text-capitalize">
                        <%=pageText.eightHours%>
                      </span>
                    </p>
                  </div>

                </div>
                <div class="col-6 col-lg-6 col-md-6 col-sm-6 view-wrapper text-center bg-success p-2">
                  <h5 class="sub-tittle text-capitalize font-400">
                    <%=pageText.rehab%>
                  </h5>
                  <div class="container-fluid p-1  text-white d-flex align-items-center justify-content-center">
                    <p>
                      <span class="font-300">
                        <%=pageText.serviceHourPerMonth%>
                      </span>
                      <span class="font-300 text-capitalize">
                        <%=pageText.eightHours%>
                      </span>
                    </p>
                  </div>

                </div>


              </div>

              <div class="col-12 view-wrapper text-center p-2 mt-2" style="background-color:floralwhite">
                <h5 class="sub-tittle text-capitalize font-400">
                  <%=pageText.case2Fee%>
                </h5>
                <div class="container-fluid p-1 text-white d-flex align-items-center justify-content-center">
                  <h6 class="font-300">
                    <%=pageText.case2MonthFee%>
                  </h6>


                </div>

              </div>

              <div class="w-100 mt-5 bg-primary text-white border border-secondary rounded-4 px-2 py-1 shadow-lg ">
                <%=pageText.baseOnUserFee%>
              </div>





            </div>
          </div>

          <div class="col-lg-5">
            <h1 class="sub-tittle text-capitalize font-800">
              <%=pageText.chargeDetail%>
            </h1>

            <p class="pera-subtitle mt-15 text-large-left">
              <span style="font-size: 16x; font-weight: 500; color: #000000;">
                <%=pageText.chargeDetailDesc%>
              </span>
              <span>
                <%=pageText.chargeDetailDescAmount%>
              </span>
            </p>

            <p class="pera-subtitle mt-15 text-large-left">
              <span style="font-size: 16x; font-weight: 500; color: #000000;">
                <%=pageText.chargeDetailDesc2%>
              </span>
              <span>
                <%=pageText.fivePercent%>
              </span>
              <span style="font-size: 16x; font-weight: 500; color: #000000;">
                <%=pageText.chargeDetailDesc3%>
              </span>
              <span>
                <%=pageText.fortyPercent%>
              </span>
              <span style="font-size: 16x; font-weight: 500; color: #000000;">
                <%=pageText.chargeDetailDesc4%>
              </span>
            </p>


            <div class="mt-5 col-12 view-wrapper text-center bg-secondary p-2 font-600">
              <p class="sub-tittle">
                <%=pageText.serviceUserPay%>
              </p>
            </div>
            <div class="col-12 view-wrapper text-center bg-light p-2 font-600">
              <p class="sub-tittle">
                <%=pageText.serviceUserPayDesc%>
              </p>
            </div>
            <div class="d-flex text-center">
              <% pageText.serviceUserPayData.forEach((item, index)=> { %>
                <div class="col-2 view-wrapper text-center px-3  py-5 font-600" style=<%='background-color:'
                  +(index%2==0?'floralwhite':'rgb(94, 198, 219)')%>
                  >
                  <h6 class="sub-tittle mb-3 font-700">
                    <%=item.percentage%>
                  </h6>
                  <p>
                    <%=item.from%>
                  </p>
                  <p>
                    ~
                  </p>
                  <p>
                    <%=item.to%>
                  </p>
                </div>
                <% }) %>
            </div>





          </div>

        </div>
    </section>
    <!-- End-of About us Area -->


    <!-- Any Question Area S t a r t -->
    <section class=" question-area bottom-padding">
      <div class="container">
        <div class="row">
          <div class="col-lg-12 my-auto">
            <!-- Section Tittle -->
            <div class="row justify-content-center">
              <div class="col-xl-7">
                <!-- Section Tittle -->
                <div class="section-tittle text-center">
                  <span class="sub-tittle text-capitalize font-600">
                    <%=pageText.couponFAQ%>
                  </span>

                </div>
              </div>
            </div>
            <div class="accordion" id="accordionExample">
              <div class="accordion-item">
                <h2 class="accordion-header" id="headingOne">
                  <button class="accordion-button collapsed additional-styles" type="button" data-bs-toggle="collapse"
                    data-bs-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                    <%=pageText.commonQuestion1%>
                  </button>
                </h2>
                <div id="collapseOne" class="accordion-collapse collapse" aria-labelledby="headingOne"
                  data-bs-parent="#accordionExample">
                  <div class="accordion-body">
                    <%=pageText.commonQuestion1Description%>
                  </div>
                </div>
              </div>
              <div class="accordion-item">
                <h2 class="accordion-header" id="headingTwo">
                  <button class="accordion-button collapsed additional-styles" type="button" data-bs-toggle="collapse"
                    data-bs-target="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                    <%=pageText.commonQuestion2%>
                  </button>

                </h2>
                <div id="collapseTwo" class="accordion-collapse collapse" aria-labelledby="headingTwo"
                  data-bs-parent="#accordionExample">
                  <div class="accordion-body">
                    <%=pageText.commonQuestion2Description%>

                  </div>
                </div>
              </div>
              <div class="accordion-item">
                <h2 class="accordion-header" id="headingThree">
                  <button class="accordion-button collapsed additional-styles" type="button" data-bs-toggle="collapse"
                    data-bs-target="#collapseThree" aria-expanded="false" aria-controls="collapseThree">
                    <%=pageText.commonQuestion3%>
                  </button>

                </h2>
                <div id="collapseThree" class="accordion-collapse collapse" aria-labelledby="headingThree"
                  data-bs-parent="#accordionExample">
                  <div class="accordion-body">
                    <%=pageText.commonQuestion3Description%>
                  </div>

                </div>
              </div>
              <div class="accordion-item">
                <h2 class="accordion-header" id="headingFour">
                  <button class="accordion-button collapsed additional-styles" type="button" data-bs-toggle="collapse"
                    data-bs-target="#collapseFour" aria-expanded="false" aria-controls="collapseFour">
                    <%=pageText.commonQuestion4%>
                  </button>
                </h2>
                <div id="collapseFour" class="accordion-collapse collapse" aria-labelledby="headingFour"
                  data-bs-parent="#accordionExample">
                  <div class="accordion-body">
                    <%=pageText.commonQuestion4Description%>
                  </div>
                </div>
              </div>
              <div class="accordion-item">
                <h2 class="accordion-header" id="headingFive">
                  <button class="accordion-button collapsed additional-styles" type="button" data-bs-toggle="collapse"
                    data-bs-target="#collapseFive" aria-expanded="false" aria-controls="collapseFive">
                    <%=pageText.commonQuestion5%>
                  </button>
                </h2>
                <div id="collapseFive" class="accordion-collapse collapse" aria-labelledby="headingFive"
                  data-bs-parent="#accordionExample">
                  <div class="accordion-body">
                    <%=pageText.commonQuestion5Description%>
                  </div>
                </div>
                <div class="accordion-item">
                  <h2 class="accordion-header" id="headingSix">
                    <button class="accordion-button collapsed additional-styles" type="button" data-bs-toggle="collapse"
                      data-bs-target="#collapseSix" aria-expanded="false" aria-controls="collapseSix">
                      <%=pageText.commonQuestion6%>
                    </button>
                  </h2>
                  <div id="collapseSix" class="accordion-collapse collapse" aria-labelledby="headingSix"
                    data-bs-parent="#accordionExample">
                    <div class="accordion-body">
                      <%=pageText.commonQuestion6Description%>
                    </div>
                  </div>

                </div>
              </div>
            </div>
          </div>
    </section>
    <!-- End-of Question Area -->


  </main>

  <!-- End-of Footer -->

  </body>

</html>